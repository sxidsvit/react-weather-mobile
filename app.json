{
  "expo": {
    "name": "React weather mobile",
    "description": "Test task from reaktivate.com",
    "slug": "react-weather-mobile",
    "privacy": "public",
    "sdkVersion": "38.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
