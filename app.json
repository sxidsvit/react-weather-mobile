      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "edu.sxidsvit.reactweathermobile",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "edu.sxidsvit.reactweathermobile",
      "versionCode": 1,
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATIONE"]
    },
